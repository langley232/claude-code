{
  "version": 2,
  "name": "atlasweb",
  "alias": ["atlasweb.info", "www.atlasweb.info"],
  "public": true,
  "github": {
    "silent": false,
    "autoAlias": true,
    "autoJobCancelation": true
  },
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/index.html"
    },
    {
      "src": "/functional-search",
      "dest": "/src/functional-search.html"
    },
    {
      "src": "/specialized-search",
      "dest": "/src/specialized-search.html"
    },
    {
      "src": "/pricing",
      "dest": "/src/pricing.html"
    },
    {
      "src": "/email-assistant",
      "dest": "/src/email-assistant.html"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|otf))",
      "dest": "/src/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/src/**/*.(js|css)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/src/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "framework": null
}
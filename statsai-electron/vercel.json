{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/index.html"
    },
    {
      "src": "/functional-search",
      "dest": "/src/functional-search.html"
    },
    {
      "src": "/specialized-search", 
      "dest": "/src/specialized-search.html"
    },
    {
      "src": "/pricing",
      "dest": "/src/pricing.html"
    },
    {
      "src": "/email-assistant",
      "dest": "/src/email-assistant.html"
    },
    {
      "src": "/registration",
      "dest": "/src/registration.html"
    },
    {
      "src": "/styles.css",
      "dest": "/src/styles.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/script.js",
      "dest": "/src/script.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/functional-search.js",
      "dest": "/src/functional-search.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/pricing.js",
      "dest": "/src/pricing.js", 
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/registration.js",
      "dest": "/src/registration.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/email-assistant.js",
      "dest": "/src/email-assistant.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/email-assistant.css",
      "dest": "/src/email-assistant.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/data/(.*)",
      "dest": "/src/data/$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|otf))",
      "dest": "/src/$1"
    }
  ]
}